[user]
	name = mumanski
	email = umanski.maciej@gmail.com
	signingKey = ssh-ed25519 AAAAC3NzaC1lZDI1NTE5AAAAIG6FnslkRbwRji2tn+NOgs53kWpEUwKhGVbD7mknCwB6
[gpg]
	format = ssh
[gpg "ssh"]
	allowedSignersFile = ~/.ssh/allowed_signers
[commit]
	gpgsign = true
	template = "~/.gitmessage"
[init]
	defaultBranch = main
[core]
	excludesfile = ~/.gitignore_global
[push]
	default = upstream
[alias]
	quickfix = !git add . && git commit -m \"fix: quickfix\" && git push
