[user]
	name = maciej-umanski
	email = umanski.maciej@gmail.com
	signingKey = ssh-ed25519 AAAAC3NzaC1lZDI1NTE5AAAAIFb9t129Jzjr7lEiCzQYmx02D75tmWk4Lu4IpmHo233R
[gpg]
	format = ssh
[gpg "ssh"]
	allowedSignersFile = ~/.ssh/allowed_signers
[commit]
	gpgsign = true
	template = "~/.gitmessage"
[init]
	defaultBranch = main
[core]
	excludesfile = ~/.gitignore_global
[push]
	default = upstream
[alias]
	quickfix = !git add . && git commit -m \"quickfix\" && git push
